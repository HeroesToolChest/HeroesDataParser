<?xml version="1.0" encoding="utf-8" ?>
<Button>
  <CButton default="1">
    <Name value="Button/Name/##id##"/>
    <Tooltip value="Button/Tooltip/##id##"/>
    <TooltipFlags index="ShowName" value="1"/>
    <TooltipFlags index="ShowHotkey" value="1"/>
    <TooltipFlags index="ShowUsage" value="1"/>
    <TooltipFlags index="ShowTime" value="1"/>
    <TooltipFlags index="ShowCooldown" value="1"/>
    <TooltipFlags index="ShowRequirements" value="1"/>
    <TooltipFlags index="ShowAutocast" value="1"/>
    <UseHotkeyLabel value="1"/>
  </CButton>
  <CButton default="1" id="StormButtonParent">
    <TooltipFlags index="ShowTime" value="0"/>
    <TooltipFlags index="ShowRequirements" value="0"/>
    <TooltipFlags index="ShowAutocast" value="0"/>
    <TooltipVitalName index="Energy" value="UI/Tooltip/Abil/Mana"/>
    <SimpleDisplayText value="Button/SimpleDisplayText/##id##"/>
  </CButton>
  <CButton default="1" id="StormButtonParentTrait" parent="StormButtonParent"/>
  <CButton default="1" id="StormButtonParentFury" parent="StormButtonParent">
    <TooltipVitalName index="Energy" value="UI/Tooltip/Abil/Fury"/>
  </CButton>
  <CButton id="Hearthstone" parent="StormButtonParent">
    <Icon value="Assets\Textures\storm_ui_icon_miscrune_1.dds"/>
  </CButton>
  <CButton id="HearthstoneNoMana" parent="StormButtonParent">
    <Icon value="Assets\Textures\storm_ui_icon_miscrune_1.dds"/>
    <HotkeyAlias value="Hearthstone"/>
  </CButton>
  <CButton id="LootSpray">
    <Icon value="Assets\Textures\storm_temp_war3_btnhealingspray.dds" />
